# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_ADMIN_ID=your_admin_id
TELEGRAM_ADMIN_USERNAME=your_admin_username

# PostgreSQL Database
DATABASE_URL=postgresql://username:password@localhost:5432/bot_marketplace
DB_HOST=localhost
DB_PORT=5432
DB_NAME=bot_marketplace
DB_USER=username
DB_PASSWORD=password

# TRON Network
TRON_NETWORK=mainnet
TRON_API_KEY=your_tron_api_key
TRON_PRIVATE_KEY=your_master_private_key

# Security
ENCRYPTION_KEY=your_32_byte_encryption_key

# Configuration
MIN_DEPOSIT_TRX=1
MIN_WITHDRAWAL_TRX=1
AMOUNT_PER_REFERRAL=10.0
REFERRAL_COMMISSION_RATE=0.10
DEPOSIT_COMMISSION_RATE=0.05
DEPOSIT_TO_MAIN_WALLET_RATE=0.9

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/bot_marketplace.log
ERROR_LOG_FILE=logs/errors.log
